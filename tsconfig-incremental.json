{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "allowJs": true,
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "rootDir": "./src",
    "outDir": "./out-incremental",
    "inlineSourceMap": true,
    "inlineSources": true,
    "incremental": true,
    "tsBuildInfoFile": "./.tsbuildinfo-incremental",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "jsx": "react",
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ],
    "types": ["node", "react", "react-dom"]
  },
  "include": [
    "src/types/**/*.ts"
  ],
  "exclude": [
    "src/app/Application.ts",
    "src/extensions/**/*",
    "src/util/**/*"
  ]
}