{
    "compileOnSave": true,
    "compilerOptions": {
        "target": "esnext",
        "module": "commonjs",
        "moduleResolution": "node",
        "allowJs": true,
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "rootDir": "./src",
        "outDir": "./out",
        "inlineSourceMap": true,
        "inlineSources": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "jsx":"react"
    },
    "files": [
      "typings.custom/index.d.ts",
      "src/main.ts",
      "src/index.ts",
      "src/renderer.tsx",
      "src/splash.ts",
      "src/extensions/index.ts"
    ]
}
