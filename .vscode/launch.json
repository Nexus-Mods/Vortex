{
  "version": "0.0.1",
  "configurations": [
    {
      "name": "Launch",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "program": "${workspaceFolder}/out/main.js",
      "stopOnEntry": false,
      "args": [],
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceRoot}/node_modules/electron/dist/electron.exe",
      },
      "runtimeArgs": [
        "."
      ],
      "env": {
        "NODE_ENV": "development",
        "SIMULATE_FS_ERRORS": "false",
        "HIGHLIGHT_I18N": "false",
        "BLUEBIRD_DEBUG": "0",
        "FORCE_ALLOW_ELEVATED_SYMLINKING": "yes"
      },
      "console": "integratedTerminal",
      "sourceMaps": true,
      "outFiles": []
    },
    {
      "name": "Launch Linux",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/out/main.js",
      "stopOnEntry": false,
      "args": [],
      "cwd": "${workspaceRoot}",
      "preLaunchTask": "prestart",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
      "runtimeArgs": [
        "."
      ],
      "env": {
        "NODE_ENV": "development",
        "BLUEBIRD_DEBUG": "0"
      },
      "console": "internalConsole",
      "sourceMaps": true,
      "outFiles": []
    },
    {
      "name": "Attach to renderer",
      "type": "node",
      "request": "attach",
      "port": 9222,
      "sourceMaps": true
    },
    {
      "name": "Debug Unit Tests",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/jest-cli/bin/jest.js",
      "args": [
        "--runInBand"
      ],
      "cwd": "${workspaceRoot}",
      "preLaunchTask": null,
      "runtimeExecutable": null,
      "runtimeArgs": [
        "--nolazy"
      ]
    }
  ]
}