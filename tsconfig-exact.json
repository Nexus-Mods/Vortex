{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "allowJs": true,
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "rootDir": "./src",
    "outDir": "./out-exact",
    "inlineSourceMap": true,
    "inlineSources": true,
    "incremental": true,
    "tsBuildInfoFile": "./.tsbuildinfo-exact",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "jsx": "react",
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ],
    "types": ["node", "react", "react-dom"]
  },
  "files": [
    "src/types/IState.ts",
    "src/types/IExtensionContext.ts",
    "src/types/IRegisteredExtension.ts",
    "src/types/shared/category.ts",
    "src/types/shared/download.ts",
    "src/types/shared/extension.ts",
    "src/types/shared/gamemode.ts",
    "src/types/shared/history.ts",
    "src/types/shared/mod.ts",
    "src/types/shared/profile.ts",
    "src/types/features/core.ts",
    "src/types/features/extension.ts",
    "src/types/features/extensionContext.ts",
    "src/types/features/game.ts",
    "src/types/features/history.ts",
    "src/types/features/loadOrder.ts",
    "src/types/features/mod.ts",
    "src/types/features/nexus.ts",
    "src/types/features/profile.ts",
    "src/types/features/util.ts"
  ]
}