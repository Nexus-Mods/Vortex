{
  "copy": [
    {
      "srcPath": "src/**/*.js",
      "skipPaths": 1
    },
    {
      "srcPath": "src/stylesheets",
      "outPath": "assets/css",
      "skipPaths": 2
    },
    {
      "srcPath": "node_modules/bootstrap-sass/assets/stylesheets/_bootstrap.scss",
      "outPath": "assets/css",
      "rename": "bootstrap.scss",
      "skipPaths": 4
    },
    {
      "srcPath": "src/index.dev.html",
      "rename": "index.html",
      "skipPaths": 1
    },
    {
      "srcPath": "extensions/games/*",
      "outPath": "bundledPlugins",
      "skipPaths": 2
    },
    {
      "srcPath": "extensions/*/index.js",
      "outPath": "bundledPlugins",
      "skipPaths": 1
    }
  ]
}
