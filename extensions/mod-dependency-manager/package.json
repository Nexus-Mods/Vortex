{
  "name": "mod-dependency-manager",
  "version": "0.0.1",
  "description": "Control mod dependencies through drag&drop",
  "main": "./out/index.js",
  "repository": "",
  "scripts": {
    "tsc": "tsc -p .",
    "webpack": "node ./node_modules/webpack/bin/webpack --config webpack.config.js --display-error-details --progress --profile --color",
    "installDev": "copyfiles -f dist/* ../../out/bundledPlugins/mod-dependency-manager",
    "prebuild": "typings prune && typings install",
    "build": "npm run tsc && npm run webpack && copyfiles -f src/stylesheets/dependency-manager.scss dist && extractInfo",
    "buildDev": "npm run build && npm run installDev"
  },
  "author": "Black Tree Gaming Ltd.",
  "license": "GPL-3.0",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "@types/fs-extra-promise": "^0.0.31",
    "@types/immutability-helper": "^2.0.15",
    "@types/minimatch": "^2.0.29",
    "@types/react": "^0.14.55",
    "@types/react-bootstrap": "^0.0.37",
    "@types/react-dom": "^0.14.19",
    "@types/react-i18next": "^1.7.5",
    "@types/react-redux": "4.4.35",
    "@types/react-router": "^2.0.41",
    "@types/webpack": "^2.2.11",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "bluebird": "^3.4.6",
    "copyfiles": "^1.0.0",
    "fs-extra-promise": "^0.4.1",
    "minimatch": "^3.0.3",
    "i18next": "^3.5.1",
    "immutability-helper": "^2.2.2",
    "json-loader": "^0.5.4",
    "modmeta-db": "file:../../external/modmeta-db",
    "vortex-api": "file:../../api",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.6",
    "react-dnd": "^2.2.4",
    "react-dom": "^15.3.2",
    "react-i18next": "^1.10.1",
    "redux-act": "1.4.0",
    "semvish": "^1.1.0",
    "webpack": "^1.13.2"
  }
}
