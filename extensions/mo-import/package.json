{
  "name": "mo-import",
  "version": "0.0.2",
  "description": "Import Mods from Mod Organizer",
  "main": "./out/index.js",
  "repository": "",
  "scripts": {
    "webpack": "node ./node_modules/webpack/bin/webpack --config webpack.config.js --progress --profile --color",
