    "@types/react-bootstrap": "^0.0.37",
    "@types/react-dom": "16.0.3",
    "@types/react-redux": "4.4.35",
    "@types/react-router": "^2.0.41",
    "copyfiles": "^2.1.1",
    "i18next": "^10.5.0",
    "immutability-helper": "^2.2.2",
    "json-loader": "^0.5.4",
    "modmeta-db": "Nexus-Mods/modmeta-db#daa8935b6e38e255ec192c908adfce35d47c0336",
